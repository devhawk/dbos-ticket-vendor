{
  "name": "ticket-vendor",
  "version": "0.0.1",
  "scripts": {
    "build": "tsc",
    "openapi": "dbos-openapi generate src/operations.ts",
    "test": "npm run db:reset && jest --randomize",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "start": "tsc && dbos-sdk start",
    "db:rollback": "knex migrate:rollback",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:reset": "npm run db:rollback && npm run db:migrate && npm run db:seed"
  },
  "devDependencies": {
    "@dbos-inc/communicator-bcrypt": "^1.1.14",
    "@dbos-inc/dbos-cloud": "^1.1.14",
    "@dbos-inc/dbos-openapi": "^1.1.14",
    "@dbos-inc/eslint-plugin": "^0.0.6",
    "@types/jest": "^29.5.12",
    "@types/seedrandom": "^3.0.8",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "seedrandom": "^3.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@dbos-inc/dbos-sdk": "^1.1.14",
    "knex": "^3.1.0",
    "liquidjs": "^10.10.1"
  }
}
